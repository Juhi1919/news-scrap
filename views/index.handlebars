<script id="article-template" type="text/x-handlebars-template">
<h2>All News Scraper</h2>



<h3>Articles</h3>

<ul>
  {{#each articles}}
    {{#unless this.sleepy}}
      <li>
        {{this.title}}
        {{this.link}}  
      </li>
    {{/unless}}
  {{/each}}
</ul>

<h3>Notes</h3>

<ul>
  {{#each notes}}
    {{#if this.sleepy}}
      <li>
        {{this.title}} 
        {{this.body}}           
      </li>
    {{/if}}
  {{/each}}
</ul>
</script>